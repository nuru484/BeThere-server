{
  "name": "bethere-server",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "node --watch --env-file .env server",
    "migrate": "node --env-file .env ./node_modules/.bin/prisma migrate dev --name init",
    "start": "node server.js",
    "deploy": "npm install && npx prisma migrate deploy && npm run seed",
    "seed": "node prisma/seed.js",
    "worker:dev": "node --env-file .env worker.js",
    "worker": "node build/worker.js",
    "studio": "node --env-file .env ./node_modules/.bin/prisma studio"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@turf/turf": "^7.2.0",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.63.0",
    "cloudinary": "^2.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "express-validator": "^7.2.1",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.10",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.0",
    "eslint": "^9.39.0",
    "globals": "^16.5.0",
    "prisma": "^7.2.0"
  }
}
